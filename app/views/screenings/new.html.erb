<%# <%= render "form"%>

<%= form_for(:screening, url:  new_check_in_screening_path, method: :get) do |f| %>
  <% Screening.questions.each_with_index do |question, index|%>
    <br>
    <br>
    <%= f.label question %>
    <br>
    <br>
    <%= f.select("response-#{index}", Screening::SCORE.keys, id: "response-#{index}") %>
    <% end %>
    <br>
    <br>
    <br>
    <br>
    <br>
    <%= f.submit %>
<% end %>